<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../../../styles/style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.warriorfit.warriorfit.SignUpController">
   <children>
      <VBox layoutX="399.0" layoutY="262.0" prefHeight="196.0" prefWidth="483.0" spacing="20.0">
         <children>
            <TextField fx:id="emailField" prefHeight="50.0" prefWidth="50.0" promptText="Email">
               <font>
                  <Font size="17.0" />
               </font></TextField>
            <TextField fx:id="usernameField" prefHeight="50.0" prefWidth="50.0" promptText="Username">
               <font>
                  <Font size="17.0" />
               </font></TextField>
            <TextField fx:id="passwordField" accessibleRole="PASSWORD_FIELD" prefHeight="50.0" prefWidth="50.0" promptText="Password">
               <font>
                  <Font size="17.0" />
               </font></TextField>
         </children>
      </VBox>
      <Button fx:id="backButton" layoutX="25.0" layoutY="23.0" onAction="#onBackButtonClick" prefHeight="88.0" prefWidth="202.0" text="Go Back">
         <font>
            <Font name="Agency FB" size="39.0" />
         </font>
      </Button>
      <Button fx:id="submitButton" layoutX="534.0" layoutY="479.0" onAction="#onSubmitButtonClick" prefHeight="68.0" prefWidth="213.0" text="Sign up">
         <font>
            <Font name="Agency FB" size="36.0" />
         </font>
      </Button>
      <Label fx:id="createText" layoutX="199.0" layoutY="23.0" prefHeight="138.0" prefWidth="882.0" text="Create an Account" textFill="WHITE">
         <font>
            <Font name="Agency FB Bold" size="81.0" />
         </font>
         <padding>
            <Insets left="200.0" />
         </padding>
      </Label>
   </children>
</AnchorPane>
