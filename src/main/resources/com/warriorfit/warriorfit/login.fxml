<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../../../styles/style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.warriorfit.warriorfit.LoginController">
    <children>
        <Button fx:id="submitButton" layoutX="540.0" layoutY="440.0" onAction="#onSubmitButtonClick" prefHeight="59.0" prefWidth="201.0" text="Submit">
            <font>
                <Font name="Agency FB" size="36.0" />
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" onAction="#onBackButtonClick" prefHeight="67.0" prefWidth="183.0" text="Go Back">
            <font>
                <Font name="Agency FB" size="39.0" />
            </font>
        </Button>
      <VBox layoutX="330.0" layoutY="274.0" prefHeight="153.0" prefWidth="620.0" spacing="20.0" stylesheets="@../../../styles/style.css">
         <children>
                <TextField fx:id="emailTextField" prefHeight="59.0" prefWidth="634.0" promptText="Email">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
                  <TextField fx:id="passwordTextField" accessibleRole="PASSWORD_FIELD" prefHeight="59.0" prefWidth="50.0" promptText="Password">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
         </children>
      </VBox>
          <Label fx:id="loginText" layoutX="567.0" layoutY="14.0" prefHeight="138.0" prefWidth="148.0" text="Login" textAlignment="CENTER" textFill="WHITE">
              <font>
                  <Font name="Agency FB Bold" size="81.0" />
              </font>
          </Label>
    </children>
</AnchorPane>
