<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../../../styles/style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.warriorfit.warriorfit.HomeController">
   <children>
      <Label fx:id="currentStatsText" layoutX="470.0" layoutY="109.0" text="Current Fitness Statistics">
         <font>
            <Font name="Agency FB" size="40.0" />
         </font>
      </Label>
      <Label fx:id="welcomeText" alignment="CENTER" layoutX="339.0" layoutY="1.0" prefHeight="109.0" prefWidth="603.0" text="Welcome Ethan" textAlignment="CENTER" textOverrun="CLIP">
         <font>
            <Font name="Agency FB Bold" size="72.0" />
         </font>
         <padding>
            <Insets top="20.0" />
         </padding>
      </Label>
      <ImageView fx:id="warriorLogo" fitHeight="238.0" fitWidth="292.0" layoutX="14.0" layoutY="468.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="startWorkoutbutton" layoutX="541.0" layoutY="647.0" mnemonicParsing="false" onAction="#onStartWorkoutButtonClick" text="Start Workout">

         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="settingsButton" layoutX="1127.0" layoutY="29.0" mnemonicParsing="false" onAction="#onSettingsButtonClick" text="Settings">

               <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="22.0" layoutY="29.0" onAction="#onBackButtonClick" prefHeight="52.0" prefWidth="108.0" text="Log out">
         <font>
            <Font name="Agency FB" size="25.0" />
         </font>
      </Button>

   </children>
</AnchorPane>
